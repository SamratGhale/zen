let z = 1;
switch z {
    0 => print "zero!";
    1 => print "one!";
    1 => print "dupe one!";
    else => print "neither!";
}
// expect: one!

let b = "apple";
switch b {
    "banana" => print "fruit: banana";
    "apple" => print "fruit: apple";
    "orange" => print "fruit: orange";
    else => print "unknown fruit";
}
// expect: fruit: apple

// block statements
let c = 0;
switch c {
    0 => {
        let zero = "zero!";
        print zero; // expect: zero!
    }
    1 => {
        print "one!";
    }
    else => {
        print "neither!";
    }
}
